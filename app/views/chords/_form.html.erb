<div class="form-group">
  <label for="chord_name">Name</label>
  <input id="chord_name" class="form-control" name="chord[name]" type="text" value="<%= @chord.name %>">
</div>
<div class="form-group">
  <label for="note">Note</label>
  <select class="form-control note" name="note[note_id]" type="text">
    <% Note.all.each do |note| %>
      <option value="<%=note.id%>"><%=note.name%></option>
      <% end %>
  </select>
</div>
  <button class="btn btn-primary"><%=button_message%></button>
