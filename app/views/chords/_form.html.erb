<div class="form-group">
  <label for="chord_name">Name</label>
  <input id="chord_name" class="form-control" name="chord[name]" type="text" value="<%= @chord.name %>">
</div>
<div class="form-group">
  <label for="note1">Note #1</label>
  <select id="note1" class="form-control note" name="note[1][note_id]" type="text">
